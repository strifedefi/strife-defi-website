<div class="front-page">
  <div class="parent" id="nav">
      <%= render "./partials/nav" %> 
  </div>

  <div>
    <div class="parent" id="main">
      <div>
      <%= image_tag("cloud.png", :alt => "rss feed", class: "pfp")  %>
      </div>
    </div>
  </div>

  <div class="content-wrapper">
  </div>
<%
=begin%>
 <div id="main-container" class="container">
  <% @articles.each do |article| %>
    <%= link_to article do%>
      <div class="col-sm-6 col-md-4">
        <div class="thumbnail">
           <%= image_tag("amc") %>
            <div class="caption">
              <h3>
                <%= article.title%>
              </h3>
            </div>
              <li>
                <%= article.body%>
              </li>
          </div>
      </div>
    <% end %>
  <% end %>
</div> 
<%
=end%>


  <%
=begin%>
 <div class="parent">
    <%= render "./partials/footer" %>
  </div> 
<%
=end%>
</div>